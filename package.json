{
  "private": true,
  "name": "entry",
  "productName": "Entry",
  "main": "src/entry_offline.js",
  "version": "1.6.7",
  "config": {
    "forge": "./forge.config.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "debug": "./node_modules/.bin/electron-forge-vscode-nix --debug=5858",
    "rebuild": "electron-rebuild -v 1.7.8",
    "dist": "build",
    "comp": "set NODE_ENV=production&&electron-compile ./src",
    "start": "electron-forge start",
    "dev": "DEBUG=* npm start",
    "test": "mocha test/default.js",
    "package": "electron-forge package",
    "dist:win": "electron-forge package --arch ia32",
    "dist:win64": "electron-forge package --arch x64",
    "pack": "electron-forge package",
    "pack:win": "electron-forge package --platform=win32",
    "pack:win32": "electron-forge package --arch=ia32 --platform=win32",
    "make": "electron-forge make",
    "make:win": "electron-forge package --platform=win32&&makensis ./build/entryx64.nsi",
    "make:win32": "electron-forge package --arch=ia32 --platform=win32&&makensis ./build/entryx86.nsi"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-babili": "0.0.12",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "electron-compile": "^6.1.3",
    "electron-compilers": "^5.5.1",
    "electron-forge": "^4.1.2",
    "electron-prebuilt-compile": "1.7.11",
    "electron-rebuild": "^1.6.0",
    "mocha": "^2.5.3",
    "rimraf": "^2.5.2",
    "spectron": "^3.2.3"
  },
  "dependencies": {
    "archiver": "^2.0.0",
    "bindings": "^1.2.1",
    "bower": "^1.8.8",
    "debug": "^2.6.3",
    "decompress": "^4.2.0",
    "electron-compile": "^6.1.3",
    "es6-promise": "^4.1.0",
    "fs-extra": "^2.1.1",
    "fstream": "^1.0.11",
    "image-size": "^0.5.1",
    "jszip": "^3.1.3",
    "lodash": "^4.17.4",
    "mime-types": "^2.1.14",
    "node-gyp": "^5.0.1",
    "object.assign": "^4.0.4",
    "q": "^1.4.1",
    "rxjs": "^5.3.0",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "tar": "2.2.1",
    "xml2js": "^0.4.17"
  }
}
